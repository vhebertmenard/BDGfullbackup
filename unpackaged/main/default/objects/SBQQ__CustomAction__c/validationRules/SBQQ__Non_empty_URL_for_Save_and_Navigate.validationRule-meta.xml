<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SBQQ__Non_empty_URL_for_Save_and_Navigate</fullName>
    <active>false</active>
    <description>Non-empty URL for Save and Navigate Custom Action.</description>
    <errorConditionFormula>AND (ISPICKVAL(SBQQ__Action__c, "Save and Navigate"), ISBLANK( SBQQ__URL__c ))</errorConditionFormula>
    <errorDisplayField>SBQQ__URL__c</errorDisplayField>
    <errorMessage>URL can't be empty for Custom Action "Save and Navigate"</errorMessage>
</ValidationRule>
