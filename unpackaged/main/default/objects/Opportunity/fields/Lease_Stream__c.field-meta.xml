<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lease_Stream__c</fullName>
    <description>Lease Stream = Rental Rate (Monthly) (Quote) * Term Length (months)</description>
    <formula>IF( ISNULL(SBQQ__PrimaryQuote__c),

 Quote_Rental_Rate__c * Term_Length__c 
,

 SBQQ__PrimaryQuote__r.Total_Monthly_Amount__c  *   SBQQ__PrimaryQuote__r.SBQQ__Opportunity2__r.Term_Length__c 

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Rental Rate (Monthly) (Quote) * Term Length (months)</inlineHelpText>
    <label>Lease Stream</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
