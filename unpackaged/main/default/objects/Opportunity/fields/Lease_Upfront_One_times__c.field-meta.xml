<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lease_Upfront_One_times__c</fullName>
    <description>Lease Upfront One-times = Upfront Project Costs (L) * Sum(Upfront Mark-up (L) + 1)</description>
    <formula>IF( ISNULL(SBQQ__PrimaryQuote__c),

(Upfront_Mark_up_L__c + 1) * Upfront_Project_Costs_L__c,

(SBQQ__PrimaryQuote__r.Upfront_Mark_up_L__c + 1) *  SBQQ__PrimaryQuote__r.Upfront_Project_Costs_L__c

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Lease Upfront One-times</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
