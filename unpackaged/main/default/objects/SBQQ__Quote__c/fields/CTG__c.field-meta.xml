<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CTG__c</fullName>
    <description>Uses the Branch or Indigenous Partnership to determine the conga templates to use</description>
    <externalId>false</externalId>
    <formula>IF(  
    OR(
       ISBLANK(SBQQ__Opportunity2__r.Indigenous_Partnership__c),
       ISBLANK(SBQQ__Opportunity2__r.Indigenous_Partnership__r.Template_Group__c)  
       ),
   SBQQ__Opportunity2__r.Branch_Name__r.Conga_Template_Group__c , LEFT(SBQQ__Opportunity2__r.Branch_Name__r.Conga_Template_Group__c, 1)+ SBQQ__Opportunity2__r.Indigenous_Partnership__r.Template_Group__c )</formula>
    <inlineHelpText>Uses the Branch or Indigenous Partnership to determine the conga templates to use</inlineHelpText>
    <label>Conga Template Group</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
