<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Markup_Install__c</fullName>
    <description>[Net Total - Total Cost / Total Cost] for all Installation quote lines.</description>
    <formula>IF( Total_Cost_Install__c = 0, 0, ( Net_Total_Install__c - Total_Cost_Install__c )/ Total_Cost_Install__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>[Net Total - Total Cost / Total Cost] for all Installation quote lines.</inlineHelpText>
    <label>Total Markup % Install</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
