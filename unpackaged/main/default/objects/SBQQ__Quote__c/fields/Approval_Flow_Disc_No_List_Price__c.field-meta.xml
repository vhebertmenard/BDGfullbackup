<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approval_Flow_Disc_No_List_Price__c</fullName>
    <description>Indicates which approval flow to run for missing List Price or discounted List Price</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Transaction_Type__c, "Sale"), 8,
IF(ISPICKVAL(Transaction_Type__c, "Disaster"), 8,
IF(ISPICKVAL(Transaction_Type__c, "Lodging"), 8,
 IF(Lowest_List_Price__c = 0 || Highest_Add_Disc__c &gt; 0, 8, 9))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates which approval flow to run for missing List Price or discounted List Price</inlineHelpText>
    <label>Approval Flow: Disc/No List Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
