<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approval_Image__c</fullName>
    <description>Displays an image if the Quote is approved</description>
    <externalId>false</externalId>
    <formula>IF(  TEXT( SBQQ__Status__c )="Approved"  ,  IMAGE("https://bdgl.sharepoint.com/sites/FC/Shared%20Documents/Approved.png", "Approved Quote") , "")</formula>
    <inlineHelpText>Displays an image if the Quote is approved</inlineHelpText>
    <label>Approval Image</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
