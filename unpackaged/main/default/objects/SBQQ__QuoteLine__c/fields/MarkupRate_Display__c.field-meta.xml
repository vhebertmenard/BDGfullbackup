<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MarkupRate_Display__c</fullName>
    <description>Used to mask actual markup rate for Daily Sublease</description>
    <formula>IF(AND(Trans_Category__c = "Sub Rental", Product_Subscription_Frequency__c = "Daily"), (Unit_Price__c-SBQQ__UnitCost__c)/SBQQ__UnitCost__c , SBQQ__MarkupRate__c)</formula>
    <inlineHelpText>Used to mask actual markup rate for Daily Sublease</inlineHelpText>
    <label>MarkupRate Display</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
