<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_Cost_Display__c</fullName>
    <description>Displays Unit Cost when lines are NOT Hidden</description>
    <formula>IF(AND(Shell__c = TRUE, SBQQ__ProductFamily__c ='VAPS'),SBQQ__UnitCost__c,(IF(Shell__c = TRUE,NULL,SBQQ__UnitCost__c)))</formula>
    <label>Unit Cost Display</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
